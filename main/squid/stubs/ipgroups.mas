<%args>
@objects
</%args>
# IP-Group list
# Used by the IP-based auth plugin to assign IP addresses to filter groups.
#
# Examples:
# Straight IP matching:
#192.168.0.1 = filter1
# Subnet matching:
#192.168.1.0/255.255.255.0 = filter1
# Range matching:
#192.168.1.0-192.168.1.255 = filter1

% foreach my $obj (@objects) {
% my $addr = $obj->{address};
% $addr =~ s:/255.255.255.255:: if (index($addr, '-') > 0);
<% $addr %>=filter<% $obj->{number} %>
% }
