<%args>
	@printers
</%args>
<%init>
use EBox::Gettext;
</%init>

% foreach my $printer (@printers) {
<Printer <% $printer->{name} %>>
Info <% $printer->{manufacturer} %> <% $printer->{model} %> <% $printer->{driver} %>
Location
DeviceURI <% $printer->{location} %>
State Idle
Accepting Yes
JobSheets none none
QuotaPeriod 0
PageLimit 0
KLimit 0
</Printer>
% }
