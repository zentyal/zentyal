#: src/EBox/Event/Watcher/Monitor.pm:144 src/EBox/Monitor.pm:78
#: src/EBox/Monitor.pm:177
msgid "Monitor"
msgstr "Монитор"

#: src/EBox/Event/Watcher/Monitor.pm:161
msgid "Notify when a certain value has reached a certain threshold"
msgstr "Сообщить, когда определенное значение достигло определенного порога"

#: src/EBox/Event/Watcher/Monitor.pm:263
msgid "All data sources are within range again"
msgstr "Все источники данных находятся снова в диапазоне"

#: src/EBox/Event/Watcher/Monitor.pm:273
#, perl-brace-format
msgid "{what} \"{dS}\" is currently {value}."
msgstr "{what} \"{dS}\" сейчас {value}."

#: src/EBox/Event/Watcher/Monitor.pm:280
#, perl-brace-format
msgid "That is within the {severity} region of {minBound} and {maxBound}. "
msgstr ""
"Значение находится в пределах {severity} зоны от {minBound} до {maxBound}."

#: src/EBox/Event/Watcher/Monitor.pm:288
#, perl-brace-format
msgid "That is above the {severity} threshold of {bound}. "
msgstr "Значение {severity} выше порога {bound}."

#: src/EBox/Event/Watcher/Monitor.pm:291
#, perl-brace-format
msgid "That is below the {severity} threshold of {bound}. "
msgstr "Значение {severity} ниже порога {bound}."

#: src/EBox/Monitor.pm:150
#, perl-brace-format
msgid "{daemon} main configuration file"
msgstr "Главный конфигурационный файл {daemon}"

#: src/EBox/Monitor.pm:155
#, perl-brace-format
msgid "{daemon} thresholds configuration file"
msgstr "Конфигурационный файл {daemon} порогов"

#: src/EBox/Monitor.pm:160
#, perl-brace-format
msgid "{daemon} default configuration file"
msgstr "Конфигурационный файл по умолчанию {daemon}"

#: src/EBox/Monitor/Measure/CPU.pm:59
#, perl-brace-format
msgid "CPU {no} usage"
