msgstr "Det er over {severity} terskelen av {bound}."

#: src/EBox/Event/Watcher/Monitor.pm:291
#, perl-brace-format
msgid "That is below the {severity} threshold of {bound}. "
msgstr "Det er under {severity} terskelen av {bound}."

