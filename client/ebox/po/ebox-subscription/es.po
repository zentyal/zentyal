msgid "FREE Basic Subscription"
msgstr "Suscripción Básica GRATIS"

#: ../ebox/src/templates/links-widget.mas:22
msgid "Subscriptions & Services"
msgstr "Suscripciones & Servicios"

#: ../ebox/src/templates/links-widget.mas:25
msgid "Commercial Subscriptions"
msgstr "Suscripciones Comerciales"

#: ../ebox/src/templates/links-widget.mas:27
msgid "Commercial Support"
msgstr "Soporte Comercial"

#: ../ebox/src/templates/links-widget.mas:29
msgid "Administer and monitor multiple Zentyal servers from a central point."
msgstr ""
"Administra y monitoriza múltiples servidores Zentyal desde un punto central."

#: ../ebox/src/templates/links-widget.mas:31
msgid "Certified Training"
msgstr "Formación Certificada"

#: ../ebox/src/templates/links-widget.mas:33
msgid "Online Store"
msgstr "Tienda Online"

